{
	"name": "@credo-js/render-driver-react",
	"version": "1.0.0",
	"author": "credo@websoftlab.com",
	"license": "MIT",
	"scripts": {
		"build": "node ../../scripts/build.js",
		"build:i": "node ../../scripts/build.js --increment-version",
		"build:i-major": "node ../../scripts/build.js --increment-version=major",
		"build:i-minor": "node ../../scripts/build.js --increment-version=minor",
		"release:local": "npm publish build --registry http://localhost:4873"
	},
	"dependencies": {
		"@credo-js/html-head": "*",
		"@credo-js/loadable": "*",
		"@credo-js/responder-page": "*",
		"@credo-js/utils": "*",
		"@credo-js/cli-debug": "*",
		"mobx": "^6.3.2",
		"mobx-react-lite": "^3.3.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^5.2.0",
		"history": "^4.9.0"
	},
	"devDependencies": {
		"@credo-js/lexicon": "*",
		"@types/history": "^4.7.9",
		"@types/react": "^17.0.16",
		"@types/react-dom": "^17.0.9",
		"@types/react-router-dom": "^5.1.8",
		"@types/node": "^14.14.31",
		"@types/koa": "^2.13.3",
		"@babel/preset-react": "^7.16.7",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
		"react-refresh": "^0.10.0"
	},
	"peerDependencies": {
		"credo": "*"
	},
	"exports": {
		"./": "./src/"
	},
	"types": "build/index.d.ts",
	"typesVersions": {
		"*": {
			"build/index.d.ts": ["src/index.ts"],
			"*": ["src/*"]
		}
	},
	"_bundle": {
		"remove": [
			"scripts",
			"bin",
			"exports",
			"types",
			"typesVersions"
		]
	}
}