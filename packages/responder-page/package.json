{
	"name": "@credo-js/responder-page",
	"version": "1.0.0",
	"author": "credo@websoftlab.com",
	"license": "MIT",
	"scripts": {
		"build": "node ../../scripts/build.js",
		"build:i": "node ../../scripts/build.js --increment-version",
		"build:i-major": "node ../../scripts/build.js --increment-version=major",
		"build:i-minor": "node ../../scripts/build.js --increment-version=minor",
		"release:local": "npm publish build --registry http://localhost:4873"
	},
	"dependencies": {
		"http-errors": "^1.7.3",
		"mobx": "^6.3.2",
		"axios": "^0.26.0",
		"@credo-js/loadable": "*",
		"@credo-js/make-url": "*",
		"@credo-js/html-head": "*",
		"@credo-js/utils": "*"
	},
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/history": "^4.7.9",
		"@types/react": "^17.0.16",
		"@types/react-dom": "^17.0.9",
		"@types/react-router-dom": "^5.1.8",
		"@types/http-errors": "^1.6.3",
		"@types/koa": "^2.13.3",
		"@types/node": "^14.14.31"
	},
	"peerDependencies": {
		"@credo-js/server": "*",
		"koa": "^2.13.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^5.2.0",
		"history": "^4.9.0",
		"mobx-react-lite": "^3.3.0"
	},
	"exports": {
		"./": "./src/"
	},
	"types": "build/index.d.ts",
	"typesVersions": {
		"*": {
			"build/index.d.ts": ["src/index.ts"],
			"*": ["src/*"]
		}
	},
	"_bundle": {
		"remove": [
			"scripts",
			"bin",
			"exports",
			"types",
			"typesVersions"
		]
	}
}